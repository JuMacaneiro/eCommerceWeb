extends admin

block details
    .table-responsive
        table.table.table-condensed
            thead
                tr
                    td #
                    td Fullname
                    td Email
                    td Username
                    td Admin
                    td
            tbody
                each customer in customers
                    tr
                        td= customer.UserID
                        td= customer.FullName
                        td= customer.Email
                        td= customer.Username
                        td= customer.Admin
                        td Update Status or View